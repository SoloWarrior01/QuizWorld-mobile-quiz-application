<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizWorld">
        <activity
            android:name=".Activity3"
            android:exported="false"
            android:label="Python Quiz" />
        <activity
            android:name=".Activity2"
            android:exported="false"
            android:label="Neural Networks Quiz" />
        <activity
            android:name=".Activity_choose"
            android:exported="false"
            android:label="Choose Category" />
        <activity
            android:name=".MainActivity_Result"
            android:exported="false"
            android:label="@string/result_txt" />
        <activity
            android:name=".Activity1"
            android:exported="false"
            android:label="@string/quiz_txt" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>